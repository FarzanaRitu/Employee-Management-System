{
    "builds": [{
        "src": "mysite/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "mysite/wsgi.py"
        }
    ],
    "ignore": [".git", "node_modules", "venv", "__pycache__", "*.log", "*.md"]
}
